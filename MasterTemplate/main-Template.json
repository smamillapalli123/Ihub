      "type": "Microsoft.Resources/deployments",
      "apiVersion": "2021-04-01",
      "name": "linkTemplateEventHub",
      "properties": {
        "mode": "Incremental",
        "templateLink": {
         "uri": "[concat(parameters('blobeventhubtemplateurl'), '?', parameters('templatesastoken'))]",
